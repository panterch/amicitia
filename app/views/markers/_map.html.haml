#operation
  %h1.accordion_toggle Recent Entries
  .accordion_content
    %ul
      -@markers.each do |marker|
        %li
          =time_ago_in_words marker.created_at
          ago:
          =link_to_function h(marker.body), "activateMarker(#{marker.id})"
      %li
        =link_to 'RSS Feed', formatted_markers_url(:rss)
  %h1.accordion_toggle Add your own entry
  .accordion_content{:style => 'display: none'}
